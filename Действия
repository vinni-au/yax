Домены
	Добавить домен		(имя, положение)
	Удалить домен		(имя, положение)
	Переименовать домен	(старое имя, новое имя)
	Переместить домен	(старое положение, новое положение)

Значения
	Добавить значение		(домен, имя, положение)
	Удалить значение		(домен, имя, положение)
	Переименовать значение	(домен, старое имя, новое имя)
	Переместить значение	(домен, старое положение, новое положение)

Переменные
	Добавить переменную			(имя, положение)
	Удалить переменную			(имя, положение)
	Переименовать переменную	(старое имя, новое имя)
	Изменить переменную			(имя, старый тип, новый тип, старый домен, новый домен, старый вопрос, новый вопрос)
	Переместить переменную		(старое положение, новое положение)

Правила
	Добавить правило		(имя, положение)
	Удалить правило			(имя, положение)
	Переименовать правило	(старое имя, новое имя)
	Изменить объяснение		(имя правила, старое объяснение, новое объяснение)
	Добавить посылку		(имя правила, положение, переменная, отношение, значение)
	Удалить посылку			(имя правила, положение, переменная, отношение, значение)
	Добавить заключение		(имя правила, положение, переменная, отношение, значение)
	Удалить заключение		(имя правила, положение, переменная, отношение, значение)
	Изменить посылку		(имя правила, номер посылки, старая переменная, новая переменная, старое отношение, новое отношение, старое значение, новое значение)
	Изменить заключение		(имя правила, номер заключения, старая переменная, новая переменная, старое отношение, новое отношение, старое значение, новое значение)
	Переместить правило		(старое положение, новое положение)
	Переместить посылку		(имя правила, старое положение, новое положение)
	Переместить заключение	(имя правила, старое положение, новое положение)
